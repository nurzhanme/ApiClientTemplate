{
  "$schema": "http://json.schemastore.org/template",
  "author": "Nurzhan Aitbayev",
  "classifications": [ "c# dotnet rest restful http api client template" ],
  "name": "Api Client Template",
  "identity": "ApiClientTemplate",
  "sourceName": "ApiClientTemplate",
  "shortName": "apiclient",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "symbols": {
	  "Framework": {
      "type": "parameter",
      "description": "The target framework to project",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net7.0"
        },
        {
          "choice": "net6.0"
        },
        {
          "choice": "netstandard2.1"
        },
        {
          "choice": "netstandard2.0"
        }
      ],
      "defaultValue": "net6.0",
      "replaces": "{TargetFramework}"
    }
  }
}